    "@vitejs/plugin-react": "^1.0.7",
    "eslint": "*",
    "husky": "*",
    "lint-staged": "*",
    "npm-run-all": "*",
    "prettier": "*",
    "typescript": "^4.5.4",
    "vite": "^2.8.0"
